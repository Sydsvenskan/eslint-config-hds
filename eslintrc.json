{
  "plugins": [
    "security",
    "mocha"
  ],
  "extends": [
    "standard",
    "plugin:security/recommended",
    "plugin:mocha/recommended",
    "@hdsydsvenskan/eslint-config-hds-core"
  ],
  "rules": {
    "mocha/no-mocha-arrows": 0,
    "node/no-deprecated-api": 1,
    "security/detect-object-injection": 0,
    "security/detect-unsafe-regex": 0
  }
}
